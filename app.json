{
  "expo": {
    "name": "GitHubJobs",
    "slug": "GitHubJobs",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/iconGit.png",
    "splash": {
      "image": "./assets/iconGitAtual.png",
      "resizeMode": "contain",
      "backgroundColor": "#F6F6F6"
    },
    "androidStatusBar": {
      "backgroundColor": "#000000",
      "translucent": true
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "location",
          "fetch"
        ]
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
